# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
indent_style = tab
tab_width = 4
trim_trailing_whitespace = true
end_of_line = lf
insert_final_newline = true

[{package.json,.travis.yml}]
indent_style = space
indent_size = 2

[*.yml]
indent_size = 2
indent_style = space
